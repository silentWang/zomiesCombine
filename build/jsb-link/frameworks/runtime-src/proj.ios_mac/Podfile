# Uncomment the next line to define a global platform for your project
source 'https://github.com/CocoaPods/Specs.git'
platform :ios, '10.0'




target 'hdwinged-mobile' do
  # Comment the next line if you don't want to use dynamic frameworks
  use_frameworks!

pod 'AnyThinkiOS','6.1.71'
pod 'AnyThinkiOS/AnyThinkUnityAdsAdapter'
pod 'AnyThinkiOS/AnyThinkFacebookAdapter'
pod 'AnyThinkiOS/AnyThinkAdmobAdapter'
pod 'Masonry'
  # Pods for hdwinged-mobile
post_install do |installer|
    installer.generated_projects.each do |project|
        project.targets.each do |target|
            target.build_configurations.each do |config|
                config.build_settings['CODE_SIGN_IDENTITY'] = '13.0'
             end
        end
      end
  end
end
